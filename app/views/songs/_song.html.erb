<% @artist.songs.each do |songs| %>
  <div class="row">
      <div class="col-md-3">
      <p>
        Title:
        <%= song.title %>
      </p>
    </div>
    <div class="col-md-3">
      <p>
        Length:
        <%= song.length %>
      </p>
    </div>
    <div class="col-md-3">
      <p>
        Year:
        <%= song.release_year %>
      </p>
    </div>
    <div class="col-md-3">
      <p>
        Album:
        <%= song.album %>
      </p>
    </div>
  </div>
<% end %>
